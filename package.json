{
  "name": "type-my-env",
  "version": "1.1.1",
  "description": "Generate typings for your .env! Remember to add the directory `types` into your `typeRoots` in your tsconfig file.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "build": "tsc && ./postbuild",
    "cbuild": "rm -rf .tsbuildinfo && rm -rf dist && npm run build",
    "cbuild:staging": "npm run cbuild && ./change-dist-build-to-staging.cjs",
    "start": "ts-node --files src/index.ts"
  },
  "bin": {
    "type-my-env": "index.js"
  },
  "keywords": [
    "dotenv"
  ],
  "author": "Ye Yu <rafolwen98@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/argparse": "^2.0.10",
    "@types/jest": "^28.1.5",
    "@types/node": "^18.0.3",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ye-yu/ts-type-my-env"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "chalk": "^5.0.1",
    "typescript": "^4.7.4"
  }
}
